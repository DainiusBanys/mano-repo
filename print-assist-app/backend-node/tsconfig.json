{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "dist",
    "rootDir": "./",
    "strict": false, // <--- CRITICAL CHANGE: Turn off strict mode
    "strictNullChecks": false, // <--- CRITICAL CHANGE: Stop complaining about nulls
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}
